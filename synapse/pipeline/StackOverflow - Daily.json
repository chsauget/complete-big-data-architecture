{
	"name": "StackOverflow - Daily",
	"properties": {
		"activities": [
			{
				"name": "Copy - Posts",
				"type": "Copy",
				"dependsOn": [],
				"policy": {
					"timeout": "0.01:00:00",
					"retry": 3,
					"retryIntervalInSeconds": 60,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"source": {
						"type": "RestSource",
						"httpRequestTimeout": "00:01:40",
						"requestInterval": "00.00:00:00.010",
						"requestMethod": "GET",
						"paginationRules": {
							"QueryParameters['urlquery']": "$.nextLink"
						}
					},
					"sink": {
						"type": "JsonSink",
						"storeSettings": {
							"type": "AzureBlobFSWriteSettings"
						},
						"formatSettings": {
							"type": "JsonWriteSettings",
							"filePattern": "setOfObjects"
						}
					},
					"enableStaging": false
				},
				"inputs": [
					{
						"referenceName": "RestStackOverflow",
						"type": "DatasetReference"
					}
				],
				"outputs": [
					{
						"referenceName": "Http2JsonBinary",
						"type": "DatasetReference",
						"parameters": {
							"FileName": {
								"value": "Posts/@formatDateTime({utcnow(),'yyyyMMdd')}/@formatDateTime({utcnow(),'hh-mm')}.json",
								"type": "Expression"
							}
						}
					}
				]
			}
		],
		"annotations": []
	}
}