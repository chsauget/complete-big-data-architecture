				"source": [
					"%%pyspark\r\n",
					"from pyspark.sql.functions import *\r\n",
					"from pyspark.sql.types import *\r\n",
					"lakepath = \"abfss://lake@synapselakeapp.dfs.core.windows.net/StackOverflow/\"\r\n",
					"rawpath = \"raw/Daily/tags/*/*.json\"\r\n",
					"goldpath = \"gold/\""
